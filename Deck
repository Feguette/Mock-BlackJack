 * @version (a version number or a date)
 */
import java.util.ArrayList;
public class Deck
{
    private ArrayList<Cards> deck = new ArrayList();
    //private String[] suits = {"S", "C", "D", "H"};
    public Deck()
    {
        for (int i = 0; i < 4; i++)
        {
            for (int j = 1; j <= 13; j++)
            {
                deck.add(new Cards(j, i));
            }
        }
    }
    
}
